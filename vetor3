programa
{
	funcao inicio()
	{
		  inteiro Mvetor [10] // declara um vetor de 10 posições
		  para (inteiro i = 0; i < 10; i++)
		  {
		  	escreva ("digite um número: ")
		  	leia (Mvetor[i])
		  }
		  escreva ("A SOMA DO VETOR É: ",somavetor (Mvetor))
	}
	funcao inteiro somavetor (inteiro v[])
	{
	inteiro Msoma
	Msoma = 0
	        para (inteiro i = 0; i < 10; i++)
	        {
	        	        Msoma = Msoma + v[i]
	        }
	         retorne Msoma
	         	         		  	        
	}        
	
		  
	
	
	
}
